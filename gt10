#
# gt10
#
#

/*sets*/
set V;
set E within (V cross V);

/*variaveis de decisoes*/
var b {(i,j) in E}, binary;


/*funcao objetivo*/
minimize z: sum{(i,j) in E} b[i,j];


/*Restricoes maximais*/
s.t. rv1{i in V}: sum{(i,j) in E} b[i,j] + sum{(j,i) in E} b[j,i] = 1;


solve;

printf: "%d", sum{(i,j) in E} b[i,j] > "gt10.out";

end;
